  "bugs": {
    "url": "https://github.com/dneprokos/playwright-test-demo/issues"
  },
  "homepage": "https://github.com/dneprokos/playwright-test-demo#readme",
  "devDependencies": {
    "@playwright/test": "^1.41.1",
    "@types/mssql": "^8.1.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "allure-playwright": "^2.0.0-beta.15",
    "dotenv": "^16.0.0",
    "eslint": "^8.12.0",
    "mssql": "^9.1.1",
    "typescript": "^5.0.2"
  }
