{
  "name": "playwright-test-demo",
  "version": "1.0.0",
  "description": "Demo test project created with playwright/test",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:ui": "npx playwright test --ui",
    "build": "tsc -b",
    "lint": "eslint . --ext .ts",
    "update": "npm install @playwright/test@latest",
    "download:chromium": "npx playwright install chromium",
    "download:firefox": "npx playwright install firefox",
    "download:webkit": "npx playwright install webkit",
    "version": "npx @playwright/test --version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dneprokos/playwright-test-demo.git"
  },
  "keywords": [
    "aqa",
    "playwright",
    "ts"
  ],
  "author": "Kostiantyn Teltov",
